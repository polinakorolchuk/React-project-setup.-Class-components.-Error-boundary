{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@services/*": ["services/*"]
    }
  },
  "include": ["src", "services"],
  "exclude": ["node_modules"],
  "types": ["node", "react", "babel__core", "babel__generator", "babel__template"]
}


  